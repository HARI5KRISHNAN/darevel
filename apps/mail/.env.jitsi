# Jitsi Configuration
# Copy this to .env or add these variables to your existing .env file

# JWT Secret - CHANGE THIS TO A RANDOM STRING IN PRODUCTION!
JITSI_JWT_SECRET=ChangeMeToRandomSecret123!

# JVB Authentication
JVB_AUTH_PASSWORD=jvbpassword123

# Jicofo Authentication
JICOFO_AUTH_PASSWORD=focuspassword123
JICOFO_COMPONENT_SECRET=componentSecret123

# Jigasi (SIP Gateway) - Optional
JIGASI_XMPP_PASSWORD=jigasipassword123

# Jibri (Recording) - Optional
JIBRI_RECORDER_PASSWORD=jibrirecorderpassword123
JIBRI_XMPP_PASSWORD=jibripassword123

# Jitsi Domain Configuration
JITSI_DOMAIN=localhost
JITSI_PORT=8000
JITSI_SECURE=false
