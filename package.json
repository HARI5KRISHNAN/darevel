{
  "name": "darevel-suite",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "packageManager": "npm@10.9.2",
  "workspaces": [
    "apps/*",
    "apps/shared",
    "packages/*"
  ],
  "scripts": {
    "start": "node scripts/start-all.js",
    "start:frontends": "turbo run dev --parallel",
    "start:infra": "cd infrastructure && docker-compose up -d",
    "setup": "node scripts/setup.js",
    "dev": "turbo run dev --parallel",
    "dev:chat": "npm run dev --workspace=chat",
    "dev:mail": "npm run dev --workspace=mailbox-ui-clone",
    "dev:sheet": "npm run dev --workspace=sheet",
    "dev:slides": "npm run dev --workspace=@darevel/slides",
    "dev:dashboard": "npm run dev --workspace=darevel-dashboard",
    "dev:suite": "npm run dev --workspace=@darevel/suite",
    "build": "turbo run build",
    "stop:infra": "cd infrastructure && docker-compose down",
    "killports": "npx kill-port 3000 3002 3003 3004 3008 8081 8082 8083 8084 8085",
    "clean": "cd infrastructure && docker-compose down -v",
    "logs": "cd infrastructure && docker-compose logs -f",
    "logs:keycloak": "docker logs -f darevel-keycloak",
    "logs:postgres": "docker logs -f darevel-postgres",
    "reset:keycloak": "cd infrastructure && reset-keycloak.bat",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Darevel Suite - Monorepo for all Darevel applications",
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "autoprefixer": "^10.4.21",
    "kill-port": "^2.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "turbo": "^2.6.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "vite": "^5.4.21"
  }
}
