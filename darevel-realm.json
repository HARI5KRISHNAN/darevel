{
  "realm": "darevel",
  "enabled": true,
  "registrationAllowed": false,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,

  "clients": [
    {
      "clientId": "darevel-client",
      "name": "Darevel Unified Client",
      "rootUrl": "",
      "adminUrl": "",
      "baseUrl": "/",

      "enabled": true,
      "publicClient": true,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": false,
      "redirectUris": [
        "http://suite.darevel.local/*",
        "http://auth.darevel.local/*",
        "http://chat.darevel.local/*",
        "http://excel.darevel.local/*",
        "http://drive.darevel.local/*",
        "http://notify.darevel.local/*",
        "http://mail.darevel.local/*",
        "http://slides.darevel.local/*",
        "http://status.darevel.local/*",
        "http://keycloak.darevel.local/*",

        "http://localhost:*/*",
        "http://127.0.0.1:*/*",
        "http://0.0.0.0:*/*",
        "http://192.168.56.1:*/*",
        "http://192.168.112.1:*/*",
        "http://192.168.29.101:*/*",
        "http://host.docker.internal:*/*",
        "http://keycloak.darevel.local/*"
      ],
      "webOrigins": [
        "http://suite.darevel.local",
        "http://auth.darevel.local",
        "http://chat.darevel.local",
        "http://excel.darevel.local",
        "http://drive.darevel.local",
        "http://notify.darevel.local",
        "http://mail.darevel.local",
        "http://slides.darevel.local",
        "http://status.darevel.local",
        "http://keycloak.darevel.local",

        "http://localhost:*",
        "http://127.0.0.1:*",
        "http://0.0.0.0:*",
        "http://192.168.29.101:*",
        "http://host.docker.internal:*"
      ],
      "attributes": {
        "post.logout.redirect.uris": "*",
        "pkce.code.challenge.method": "S256"
      },
      "protocol": "openid-connect"
    }
  ],

  "roles": {
    "realm": [
      { "name": "user" },
      { "name": "admin" }
    ]
  },

  "groups": [],
  
  "users": [
    {
      "username": "admin",
      "enabled": true,
      "emailVerified": true,
      "firstName": "Darevel",
      "lastName": "Admin",
      "credentials": [
        {
          "type": "password",
          "value": "admin",
          "temporary": false
        }
      ],
      "realmRoles": [
        "admin"
      ]
    }
  ]
}
